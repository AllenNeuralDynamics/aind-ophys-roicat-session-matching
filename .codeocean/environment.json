{
	"version": 1,
	"base_image": "codeocean/pytorch:2.1.0-cuda11.8-mambaforge23.1.0-4-python3.10.12-ubuntu22.04",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"post_install": true,
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "build-essential",
					"version": "12.9ubuntu3"
				}
			],
			"install_software_properties_common": true
		},
		"pip3": {
			"packages": [
				{
					"name": "aind-data-schema",
					"version": "1.0.0"
				},
				{
					"name": "aind-metadata-upgrader",
					"version": "0.0.20"
				},
				{
					"name": "h5py",
					"version": "3.12.1"
				},
				{
					"name": "pint",
					"version": "0.24.3"
				},
				{
					"name": "roicat[all]",
					"version": "1.3.2"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}